{
  "name": "@coolplaylin/genshinimpact-seewonote5",
  "version": "1.1.0",
  "description": "Make your EasiNote5 especial 让你的希沃白板 5 与众不同",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build:code": "unbuild",
    "build": "run-s build:code",
    "format": "prettier --write src",
    "debug": "esno src/main.ts",
    "prepublishOnly": "run-s format build",
    "release": "bumpp"
  },
  "bin": {
    "gise": "bin/gise.mjs"
  },
  "files": [
    "public",
    "dist",
    "bin"
  ],
  "keywords": [
    "cli",
    "typescript",
    "yarn",
    "genshin-impact",
    "genshin-impact-hack"
  ],
  "author": "CoolPlayLin<CoolPlayLin@foxmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/prompts": "^2.4.9",
    "bumpp": "^9.2.1",
    "esno": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "private": false,
  "dependencies": {
    "prompts": "^2.4.2"
  }
}
