{
  "name": "genshinImpact-Seewonote5",
  "version": "0.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build:code": "tsup-node",
    "build:dist": "pkg . --openssl-no-asm",
    "build": "run-s build:code build:dist",
    "format": "prettier --write src",
    "debug": "esno src/main.ts"
