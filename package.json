  "devDependencies": {
    "@electron/packager": "^18.1.3",
    "@types/node": "^20.10.6",
    "@types/prompts": "^2.4.9",
    "esno": "^4.0.0",
    "nexe": "^4.0.0-rc.3",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.1",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  }
